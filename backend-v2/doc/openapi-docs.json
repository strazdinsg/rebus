{
  "openapi": "3.0.0",
  "info": {
    "title": "Backend API documentation",
    "version": "2.2.0",
    "description": "Rebus backend API documentation"
  },
  "paths": {
    "/challenges": {
      "get": {
        "description": "Get all challenges",
        "responses": {
          "200": {
            "description": "Successfully fetched all challenges",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": [
                        "SUCCESS",
                        "ERROR"
                      ]
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/ChallengeDto"
                      }
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ChallengeDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "question": {
            "type": "string"
          },
          "maxScore": {
            "type": "integer"
          }
        }
      }
    }
  },
  "tags": []
}
