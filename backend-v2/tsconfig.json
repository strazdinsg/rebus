{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "esnext"
    ],
    "strict": true,
    "module": "esnext",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
